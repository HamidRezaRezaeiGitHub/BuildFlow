# Test Environment Configuration
# Used when running: npm test

# Application Mode
VITE_APP_MODE=integrated
# Options: 'standalone' | 'integrated'

# API Configuration
VITE_API_BASE_URL=http://localhost:8080/api
# Test environment uses mock backend API

# Backend Integration
VITE_BACKEND_ENABLED=true
# Backend integration enabled for testing

# Feature Flags
VITE_ENABLE_MOCK_AUTH=false
VITE_ENABLE_MOCK_DATA=false
VITE_ENABLE_CONSOLE_LOGS=true
# Console logs enabled for test debugging

# Environment Name
VITE_ENVIRONMENT=development
